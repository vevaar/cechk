{
    "manifest_version": 3,
    "name": "yt killer",
    "version": "1.0",
    "description": "Performs a random Google search in the current tab.",
    "permissions": [
        "activeTab",
        "scripting",
        "webNavigation"
      ],
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["content.js"]
        }
      ]
  }
  